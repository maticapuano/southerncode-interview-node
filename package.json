{
  "name": "southerncode-interview-node",
  "version": "1.0.0",
  "main": "dist/shared/infrastructure/http/server.js",
  "repository": "git@github.com:maticapuano/southerncode-interview-node.git",
  "author": "Matias Capuano <maticapuano97@gmail.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run build && node -r dotenv/config dist/shared/infrastructure/http/server.js",
    "start:dev": "nodemon",
    "build": "npm run clean && swc src -d dist --source-maps",
    "typeorm": "npm run build && ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli",
    "migration:create": "cross-var ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/shared/infrastructure/persistence/typeorm/migrations/$npm_config_name",
    "migration:up": "npm run typeorm -- migration:run",
    "build:watch": "swc src -d dist --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src/**/*.ts --ext .ts --fix",
    "lint:all": "npm run lint && npm run format",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "env-var": "^7.3.1",
    "pg": "^8.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.74",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "cross-var": "^1.1.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
